<h4>Historial</h4>

<%= render "histories/search_form" %>

<%= link_to t('actions.create', resource: 'historia'), new_history_path(patient_id: patient) %>

<% @histories.no_pending.each do |history| %>

  <br>
  <%= render "shared/history", history: history%>

<% end %>


<% if histories(patient).empty? %>
  <div class="panel" >
    No hay datos
  </div>

<% end %>
